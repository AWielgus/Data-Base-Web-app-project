sudo apt install openfortivpn build-essensial!!!!!!! git

 sudo apt install qt5-qmake build-essential
sudo apt install build-essential libssl-dev qt5keychain-dev
sudo apt-get install qtbase5-dev qtchooser qt5-qmake qtbase5-dev-tools

// install openfortigui from source
mkdir tools
cd tools
git clone https://github.com/theinvisible/openfortigui.git
cd openfortigui
git submodule init
git submodule update
qmake && make -j4
sudo cp openfortigui/app-entry/openfortigui.png /usr/share/pixmaps
sudo cp openfortigui/app-entry/openfortigui.desktop /usr/share/applications
sudo cp openfortigui/openfortigui /usr/bin/

//
sudo apt install yakuake

//flatpak
sudo apt install flatpak
flatpak remote-add --if-not-exists flathub https://flathub.org/repo/flathub.flatpakrepo
sudo apt install plasma-discover-backend-flatpak









